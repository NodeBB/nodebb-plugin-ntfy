{
  "name": "nodebb-plugin-ntfy",
  "version": "1.1.0",
  "description": "A starter kit for quickly creating NodeBB plugins",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodebb/nodebb-plugin-ntfy"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "ntfy",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodebb/nodebb-plugin-ntfy/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^2.8.x || ^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-angular": "17.6.7",
    "eslint": "8.45.0",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "lint-staged": "13.2.3"
  },
  "dependencies": {
    "eventsource": "^2.0.2",
    "node-fetch": "^2",
    "validator": "^13.7.0"
  }
}
